{
  "name": "agent",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "bin": "build/index.js",
  "pkg": {
    "assets": [
      ".env"
    ],
    "scripts": [
      "build/socket.js",
      "build/utils.js"
    ]
  },
  "scripts": {
    "build": "npm run build-index && npm run build-socket && npm run build-utils",
    "build-index": "babel index.js -o ./build/index.js",
    "build-socket": "babel socket -o ./build/socket.js",
    "build-utils": "babel utils -o ./build/utils.js",
    "clean": "rm -rf build dist && mkdir build dist",
    "dev": "npx cross-env NODE_ENV=development nodemon --exec babel-node index.js",
    "lint": "eslint **/*.js --ignore-path .gitignore",
    "pack": "npm run clean && npm run build && pkg . --out-path ./dist -t node8-win-x64"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "nodemon": "^1.18.10"
  },
  "dependencies": {
    "dotenv": "^7.0.0",
    "socket.io-client": "^2.2.0"
  }
}
